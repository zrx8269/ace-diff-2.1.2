{
  "name": "ace-diff",
  "version": "0.0.0-development",
  "description": "A diff/merging wrapper for Ace Editor built on google-diff-match-patch",
  "main": "dist/ace-diff.min.js",
  "module": "src/index.js",
  "unpkg": "dist/ace-diff.min.js",
  "scripts": {
    "build": "webpack",
    "dev": "webpack --watch",
    "serve": "npm run build && http-server . -p 8081 -s -c-1",
    "test": "NODE_ENV=test cypress run && npm run mocha",
    "mocha": "nyc --reporter=html mocha test/**/*.test.js",
    "cypress": "cypress open",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ace-diff/ace-diff.git"
  },
  "keywords": [
    "ace",
    "dif",
    "diffing",
    "merge",
    "merging",
    "ace editor",
    "syntax highlighting"
  ],
  "author": "Ben Keen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ace-diff/ace-diff/issues"
  },
  "homepage": "https://ace-diff.github.io/ace-diff/",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.9",
    "cypress": "^1.4.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "http-server": "^0.11.1",
    "mocha": "^5.0.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "postcss-loader": "^2.0.10",
    "sass-loader": "^6.0.6",
    "semantic-release": "^12.2.2",
    "sinon": "^4.1.6",
    "travis-deploy-once": "^4.3.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "lodash": "^4.17.4"
  }
}
